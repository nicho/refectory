<?xml version="1.0" encoding="UTF-8"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client"
	xmlns:d='urn:import:com.google.gwt.user.datepicker.client'>
	<ui:image resource="input_text.jpg" field="input_text"
		repeatStyle="Horizontal"></ui:image>
	<ui:image resource="button_addpe.jpg" field="button_addpe"></ui:image>
	<ui:style src="staffBlock.css"></ui:style>
	<ui:style src="new2.css" field="new2"></ui:style>
	<g:HTMLPanel ui:field="select1">
		<div class="{new2.content-wrap}">
			<div class="{new2.form}">
				<div class="{new2.form-line}">

					<g:InlineLabel text="合格人选：" ui:field="topName"
						styleName="{new2.label}" />
					<div class="{new2.element}">
						<g:FlowPanel ui:field="staffTextAreaPanel" />	
						<span class="{new2.nbsp}"></span>
						<span class="{new2.btn}" >
							<g:Button ui:field="chooseBtn" text="选择"></g:Button>
						</span>
						<div style="clear:both"></div>
					</div>
				</div>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>