<?xml version="1.0" encoding="UTF-8"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client"
	xmlns:d='urn:import:com.google.gwt.user.datepicker.client'>

	<ui:image resource="input_text.jpg" field="input_text"
		repeatStyle="Horizontal"></ui:image>
	<ui:image resource="preview_bg.jpg" field="preview_bg"
		repeatStyle="Horizontal"></ui:image>
	<ui:image field="post_bg" resource="post_bg.jpg" repeatStyle="Horizontal"></ui:image>
	<ui:style src="rewardsItem.css" field="item"></ui:style>
	<ui:style src="new2.css" field="new2"></ui:style>
	<g:HTMLPanel>
		<div class="{new2.frame-wrap}">
			<!--面包屑 [[ -->
			<g:FlowPanel ui:field="breadCrumbs" />
			<!--面包屑 ]] -->

			
							<h3>请为奖项提名
				</h3>
				<span class="{new2.cutline}"></span>

			 <div class="{new2.content-wrap}">
			    <div class="{new2.form}" >
			      <div class="{new2.form-line}">
			        <label class="{new2.label}">奖项名称：</label>
			        <div class="{new2.element}">
			          <g:Label text="等待数据加载..." ui:field="name" />
			        </div>
			      </div>
			      			      <div class="{new2.form-line}">
			        <label class="{new2.label2}">奖项说明：</label>
			        <div class="{new2.element}">
			          <g:Label text="等待数据加载..." ui:field="explain" />
			        </div>
			      </div>
						      			      <div class="{new2.form-line}">
			        <label class="{new2.label}">获奖条件：</label>
			        <div class="{new2.element}">
			          <g:Label text="等待数据加载..." ui:field="condition" />
			        </div>
			      </div>
			      			      			      <div class="{new2.form-line}">
			        <label class="{new2.label}">奖项总积分：</label>
			        <div class="{new2.element}">
			          <g:Label text="等待数据加载..." ui:field="integral" />
			        </div>
			      </div>
			      			      			      <div class="{new2.form-line}">
			        <label class="{new2.label}">提名奖项：</label>
			        <div class="{new2.element}">
			        		项目奖，共
							<g:InlineLabel text="等待数据加载..." ui:field="number" />
							名，每人奖励
							<g:InlineLabel text="等待数据加载..." ui:field="awardAmt"  />
							积分
			        </div>
			      </div>
			     			      			      			      <div class="{new2.form-line}">
			        <label class="{new2.label}">创建人：</label>
			        <div class="{new2.element}">
			          <g:Label text="等待数据加载..." ui:field="recordName" />
			        </div>
			      </div>
			      
			      			      			      			      <div class="{new2.form-line}">
			        <label class="{new2.label}">提名人：</label>
			        <div class="{new2.element}">
			          <g:VerticalPanel ui:field="judge" />
			        </div>
			      </div>
			  		<div class="{new2.form-line}">
						<g:FlowPanel ui:field="staffPanel" />
			       </div>			
			     
			     			     			      			      			      <div class="{new2.form-line}">
			        <label class="{new2.label}">奖项模式：</label>
			        <div class="{new2.element}">
			          <g:Label text="等待数据加载..." ui:field="awardNature" />
			        </div>
			      </div>
			      			     			      			      			      <div class="{new2.form-line}">
			        <label class="{new2.label}">开始时间：</label>
			        <div class="{new2.element}">
			          <g:Label text="等待数据加载..." ui:field="begindate" />
			        </div>
			      </div>
			      			     			      			      			      <div class="{new2.form-line}">
			        <label class="{new2.label}">预计颁奖时间：</label>
			        <div class="{new2.element}">
			          <g:Label text="等待数据加载..." ui:field="awarddate" />
			        </div>
			      </div>
			      			     			      			      			      <div class="{new2.form-line}">
			        <label class="{new2.label}">提名：</label>
			        <div class="{new2.element}">
			          		<g:InlineLabel text="等待数据加载..." ui:field="nominateMessage" />
							提名时间:
							<g:InlineLabel text="等待数据加载..." ui:field="expectNominateDate" />
							提名人：
							<g:InlineLabel text="等待数据加载..." ui:field="nominateStaff" />
			        </div>
			      </div>
			     
			     			      			     			      			      			      <div class="{new2.form-line}">
			        <label class="{new2.label}">颁奖人：</label>
			        <div class="{new2.element}">
			          <g:Label text="等待数据加载..." ui:field="awardName" />
			        </div>
			      </div>
	
			     <span class="{new2.cutline}"></span>
			      <div class="{new2.form-line} {new2.form-line-btn}">
			        <div class="{new2.element}">
			          <span class="{new2.btn}"><g:Button ui:field="nominatebutton" text="提交" /></span>
			        </div>
			      </div>
			    </div>
			  </div>
			
	
</div>
	</g:HTMLPanel>
</ui:UiBinder> 
