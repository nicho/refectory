<?xml version="1.0" encoding="UTF-8"?>
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:d='urn:import:com.google.gwt.user.datepicker.client'
	xmlns:c="urn:import:com.google.gwt.user.cellview.client">

	<ui:style src="new2.css" field="new2"></ui:style>
	<g:HTMLPanel>

		<div class="{new2.frame-wrap}" style="display:none;">
			<!--面包屑 [[ -->
			<g:FlowPanel ui:field="breadCrumbs" />
			<!--面包屑 ]] -->
		</div>
		<div class="{new2.employee}">

			<div class="{new2.content-wrap}">
				<div class="{new2.content-info}">
					<div class="{new2.breadcrumb}">
						<a href="#">首页</a>
						&gt;
						<span>奖品商城</span>
					</div>
				</div>
		



			<!--查询条件 [[ -->
			<div class="{new2.query-wrap}" style="display:none;">
				<div class="{new2.form}">

					<span class="{new2.nbsp}"></span>
					<g:TextBox maxLength="20" ui:field="keyName" styleName="{new2.text}" />

					<span class="{new2.btn}">
						<g:Button ui:field="searchBtn" text="查询" />
					</span>

				</div>
			</div>
			<!--查询条件 ]] -->

			<div class="{new2.gift-wrap}">
				<p>
					共
					<span class="{new2.color-red}">
						<g:InlineLabel ui:field="dataCount" />
					</span>
					份礼品
				</p>


				<g:FlowPanel ui:field="resultPanel" />



				<!--分页 [[ -->
				<div id="rewardlist_page2" class="{new2.pager}">
					<div class="{new2.pager-left}">
						每页显示条数
						<select class="{new2.select}">
							<option>12</option>
						</select>
					</div>

					<div class="{new2.pager-right}">
						<g:FlowPanel ui:field="resultpage" />
					</div>
				</div>
				<!--分页 ]] -->

			</div>
		</div>	</div>
	</g:HTMLPanel>
</ui:UiBinder>