<?xml version="1.0" encoding="UTF-8"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client"
	xmlns:d='urn:import:com.google.gwt.user.datepicker.client'>
	<ui:style src="new2.css" field="new2"></ui:style>
	<g:HTMLPanel>
	<div class="{new2.employee}">
	     <div class="{new2.workmate-list}">
	     <div class="{new2.workmate}" ><!--IE6,鼠标滑过添加类workmate-on-->
            <div class="{new2.workmate-show}">
              <g:Image ui:field="photo" pixelSize="72,72" />
              <div class="{new2.workmate-info}">
                <p class="{new2.workmate-name}"><g:Anchor ui:field="staffName" /> </p>
                <p><g:InlineLabel ui:field="deptName" /> </p>
                <p></p>
              </div>
            </div>
            <div class="{new2.workmate-comm}">
            <g:Anchor ui:field="quietly" text="悄悄话" />
            <g:Anchor ui:field="dalliance" text="调戏他" />
            <g:Anchor ui:field="email" text="发邮件" />
           
            </div>
          </div>
          
</div></div>
	</g:HTMLPanel>
</ui:UiBinder> 