<?xml version="1.0" encoding="UTF-8"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client"
	xmlns:d='urn:import:com.google.gwt.user.datepicker.client'>
	<ui:style src="new2.css" field="new2"></ui:style>
	<g:HTMLPanel>
		<div class="{new2.employee}">
			<div class="{new2.encash}">
				<div class="{new2.gift-list} {new2.clearfix}">
					<div class="{new2.gift}">
						<g:Image ui:field="shopPhoto" pixelSize="80,60" />
						<p><g:Anchor ui:field="shopName" /> </p>
						<p>
							<span class="{new2.color-orange}"><g:InlineLabel ui:field="integral" /></span>
							积分
						</p>
					</div>
				</div>
			</div>

		</div>
	</g:HTMLPanel>
</ui:UiBinder> 