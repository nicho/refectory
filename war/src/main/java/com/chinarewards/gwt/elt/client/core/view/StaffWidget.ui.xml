	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:image field="logo" resource="logo.jpg"></ui:image>
	<ui:image field="logo_name" resource="logo_name.jpg"></ui:image>
	<ui:image field="help" resource="help.jpg"></ui:image>
	<ui:image field="helpover" resource="helpover.jpg"></ui:image>
	<ui:image field="helpdown" resource="helpdown.jpg"></ui:image>
